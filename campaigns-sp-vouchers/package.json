{
	"name": "directus-extension-vouchers",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-custom-endpoint"
	],
	"directus:extension": {
		"type": "endpoint",
		"path": "../extensions/endpoints/vouchers/index.js",
		"source": "src/index.ts",
		"host": "^9.18.1"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"watch": "npm-watch"
	},
	"watch": {
		"build": {
			"patterns": [
				"src"
			],
			"extensions": "ts"
		}
	},
	"devDependencies": {
		"@directus/extensions-sdk": "9.18.1",
		"@types/node": "^18.14.0",
		"typescript": "^4.9.5"
	},
	"dependencies": {
		"axios": "^1.3.6"
	}
}
